// C++ program where we check if a person is eligible to vote or not
#include<iostream>
using namespace std;

int main() {

    int age;
    cout << "Enter age of user:";
    cin>>age;

    if (age > 17) {
        cout << "You are eligible for voting"<<endl;
    } else {
        cout << "You are not eligible for voting"<<endl;
    }

    return 0;
}
